user              www www;
worker_processes  1;
error_log         /var/log/nginx.error.log  info;
pid               /var/run/nginx.pid;

events {
    worker_connections  1024;
}

http {
    include            mime.types;
    default_type       application/octet-stream;
    sendfile           on;
    keepalive_timeout  65;
    gzip               on;
    include            /usr/local/etc/nginx/conf.d/*.conf;
}
